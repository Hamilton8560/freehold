{
  "name": "@freehold/ui",
  "version": "0.0.4",
  "description": "Freehold UI component library",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Hamilton8560/freehold.git",
    "directory": "packages/ui"
  },
  "keywords": [
    "react",
    "components",
    "tailwindcss",
    "design-system",
    "ui"
  ],
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./charts": {
      "types": "./dist/charts.d.ts",
      "import": "./dist/charts.mjs",
      "require": "./dist/charts.js"
    },
    "./kanban": {
      "types": "./dist/kanban.d.ts",
      "import": "./dist/kanban.mjs",
      "require": "./dist/kanban.js"
    },
    "./styles.css": "./dist/index.css"
  },
  "files": [
    "dist",
    "LICENSE",
    "README.md"
  ],
  "sideEffects": [
    "**/*.css"
  ],
  "scripts": {
    "build": "tsup && node scripts/add-use-client.mjs",
    "dev": "tsup --watch",
    "clean": "rm -rf dist",
    "lint": "eslint src/",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "peerDependencies": {
    "@dnd-kit/core": "^6.3.0",
    "@dnd-kit/sortable": "^10.0.0",
    "@dnd-kit/utilities": "^3.2.0",
    "react": "^18.0.0 || ^19.0.0",
    "react-dom": "^18.0.0 || ^19.0.0",
    "recharts": "^2.12.0"
  },
  "peerDependenciesMeta": {
    "@dnd-kit/core": {
      "optional": true
    },
    "@dnd-kit/sortable": {
      "optional": true
    },
    "@dnd-kit/utilities": {
      "optional": true
    },
    "recharts": {
      "optional": true
    }
  },
  "dependencies": {
    "@radix-ui/react-dialog": "^1.1.4",
    "@radix-ui/react-popover": "^1.1.15",
    "@radix-ui/react-radio-group": "^1.3.8",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-slider": "^1.3.6",
    "@radix-ui/react-slot": "^1.2.4",
    "@radix-ui/react-toast": "^1.2.4",
    "@radix-ui/react-tooltip": "^1.2.8",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "tailwind-merge": "^2.6.0"
  },
  "devDependencies": {
    "@storybook/addon-essentials": "^8.4.0",
    "@storybook/addon-interactions": "^8.4.0",
    "@storybook/addon-links": "^8.4.0",
    "@storybook/blocks": "^8.4.0",
    "@storybook/react": "^8.4.0",
    "@storybook/react-vite": "^8.4.0",
    "@storybook/test": "^8.4.0",
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0",
    "autoprefixer": "^10.4.20",
    "postcss": "^8.4.49",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "recharts": "^2.15.0",
    "storybook": "^8.4.0",
    "tailwindcss": "^3.4.0",
    "tsup": "^8.3.5",
    "typescript": "^5.7.2",
    "vite": "^6.0.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
